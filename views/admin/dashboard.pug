extends layout

block content
  h1(class="mainText text-center") Dashboard
  div(class="container")
    div(class="row")
      div(class="col-md-6")
        h3(class="text-center") Wedding RSVPs
        hr
        ol
          each wedding in weddingRsvps
            a(href="/admin/showwedding/" + wedding.id)
              li= wedding.fullName
      div(class="col-md-6")
        h3(class="text-center") Baby Shower RSVPs
        hr
        ol
          each baby in babyRsvps
            a(href="/admin/showbaby/" + baby.id)
              li= baby.fullName
  div(id="margy")